@model IEnumerable<Vidly.Models.movie>
@{
    ViewBag.Title = "MovieList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>MovieList</h2>
<table>
    <tr>
        <th>
            Movie
        </th>
    </tr>
    @foreach(var movie in Model)
    {
        <tr>
            <td>
                @movie.name
            </td>
        </tr>
    }
    <tr>
    </tr>
</table>

